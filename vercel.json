{
    "version": 2,
    "functions": {
        "index.js": {
            "maxDuration": 60
        }
    },
    "builds": [
        { 
            "src": "./index.js", 
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "/" 
        }
    ]

}